<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:TRAPP"
             x:Class="TRAPP.HomePage">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Icon="add.png" Text="Add" Clicked="ToolbarItem_Clicked"/>
    </TabbedPage.ToolbarItems>
    <local:HistoryPage Icon="home.png" Title="History"/>
    <local:MapPage Icon="map.png" Title="Map"/>
    <local:ProfilePage Icon="person.png" Title="Profile"/>
</TabbedPage>